{
  "name": "quiz",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nodemon --exec npx ts-node ./server/app.ts",
    "dev": "concurrently --kill-others \"ng s\" \"node --inspect=0.0.0.0:9229 --require ts-node/register server/app.ts\"",
    "start-debug": "node --inspect=0.0.0.0:9229 --require ts-node/register app.ts",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.0",
    "mongoose": "^6.4.6",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.6.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.6.4"
  }
}
